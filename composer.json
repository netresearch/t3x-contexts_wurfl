{
  "name": "netresearch/contexts_wurfl",
  "type": "typo3-cms-extension",
  "description": "Multi-channel contexts: WURFL mobile data provider (Mobile device detection for multi-channel contexts)",
  "keywords": [
    "TYPO3",
    "extension",
    "contexts_geolocation",
    "contexts"
  ],
  "homepage": "https://www.netresearch.de",
  "support": {
    "issues": "https://github.com/netresearch/t3x-contexts_wurfl/issues"
  },
  "authors": [
    {
      "name": "Andre HÃ¤hnel, Christian Opitz, Christian Weiske, Marian Pollzien, Rico Sonntag",
      "email": "typo3.org@netresearch.de",
      "role": "Developer",
      "homepage": "https://www.netresearch.de"
    }
  ],
  "license": "GPL-2.0+",
  "require": {
    "netresearch/contexts": "^0.4"
  },
  "require-dev": {
    "namelesscoder/typo3-repository-client": "^1.2",
    "mikey179/vfsStream": "^1.4",
    "phpunit/phpunit": "^4.7 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Netresearch\\ContextsWurfl\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Netresearch\\ContextsWurfl\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "replace": {
    "contexts_wurfl": "self.version",
    "typo3-ter/contexts_wurfl": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "typo3/cms": "source"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/contexts_wurfl ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/contexts_wurfl"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
